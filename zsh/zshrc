#!/usr/bin/env zsh

# zmodload zsh/zprof

export EDITOR="nvim"

export DOTFILE_DIR=${ZDOTDIR:-$HOME}/.dotfiles
export ZSHCONFIG=$DOTFILE_DIR/zsh

ZSH_INIT=${ZSHCONFIG}/_init.sh

if [[ -s ${ZSH_INIT} ]]; then
    source ${ZSH_INIT}
else
    echo "Could not find the init script ${ZSH_INIT}"
fi

#
# https://gist.github.com/ctechols/ca1035271ad134841284
# https://carlosbecker.com/posts/speeding-up-zsh
#
autoload -Uz compinit

case $SYSTEM in
  Darwin)
    if [ $(date +'%j') != $(/usr/bin/stat -f '%Sm' -t '%j' ${ZDOTDIR:-$HOME}/.zcompdump) ]; then
      compinit;
    else
      compinit -C;
    fi
    ;;
  Linux)
    # not yet match GNU & BSD stat
  ;;
esac
# The following lines have been added by Docker Desktop to enable Docker CLI completions.
fpath=(/Users/macbench/.docker/completions $fpath)
autoload -Uz compinit
compinit
# End of Docker CLI completions

### MANAGED BY RANCHER DESKTOP START (DO NOT EDIT)
export PATH="/Users/macbench/.rd/bin:$PATH"
### MANAGED BY RANCHER DESKTOP END (DO NOT EDIT)
